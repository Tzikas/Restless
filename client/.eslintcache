rovider value={{ history, user, setUser, socket }}>\n      {user?.email}\n      <nav>\n        <NavLink to=\"/home\"> Home </NavLink>\n\n        {user ? (\n          <Fragment>\n            <NavLink onClick={logOut} to=\"/\">\n              Log Out\n            </NavLink>\n            <NavLink to=\"/profile\"> Profile </NavLink>\n          </Fragment>\n        ) : (\n            <Fragment>\n              {!user && <SignUp setUser={setUser} socket={socket} />}\n              {!user && <LogIn setUser={setUser} socket={socket} />}\n            </Fragment>\n          )}\n      </nav>\n      <Switch>\n        <Route\n          exact\n          path=\"/home\"\n          render={(props) => <Home {...props} user={user} />}\n        />\n\n\n        <Route\n          exact\n          path=\"/profile\"\n          render={(props) => <Profile {...props} />}\n        />\n\n        <Route component={NotFound} />\n      </Switch>\n\n      <NotificationContainer />\n    </TheContext.Provider>\n  );\n};\nexport default App;\n","/Users/niko/Documents/Code/KATAROYALE/royale/client/src/reportWebVitals.js",[],["46","47"],"/Users/niko/Documents/Code/KATAROYALE/royale/client/src/components/home/Home.js",["48","49"],"/Users/niko/Documents/Code/KATAROYALE/royale/client/src/components/profile/Profile.js",["50","51","52","53","54"],"/Users/niko/Documents/Code/KATAROYALE/royale/client/src/TheContext.js",[],"/Users/niko/Documents/Code/KATAROYALE/royale/client/src/components/auth/SignUp.js",[],"/Users/niko/Documents/Code/KATAROYALE/royale/client/src/components/auth/LogIn.js",[],"/Users/niko/Documents/Code/KATAROYALE/royale/client/src/components/404/NotFound.js",[],["55","56"],{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","severity":1,"message":"62","line":1,"column":17,"nodeType":"63","messageId":"64","endLine":1,"endColumn":26},{"ruleId":"61","severity":1,"message":"65","line":14,"column":3,"nodeType":"63","messageId":"64","endLine":14,"endColumn":22},{"ruleI